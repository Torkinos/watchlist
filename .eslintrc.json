{
  "extends": ["prettier"],
  "plugins": ["prettier", "next/core-web-vitals", "unused-imports"],
  "rules": {
    "prettier/prettier": ["error"],
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
